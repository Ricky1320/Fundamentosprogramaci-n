#include <iostream>   // libreria para manejar mensajes cout y cin

using namespace std; // maneja la salida y la entrada de standard

int main() //inicio del algoritmo
{
    cout<<"Bolivianos el hado propicio"<<endl;
    cout<<"corono nuestros votos y anhelos"<<endl;
    cout<<"es ya libre,ya libre, este suelo"<<endl;
    cout<<"ya ceso su servir condicion"<<endl;
    cout<<"al estruendo marcial que ayer fuera"<<endl;
    cout<<"y al clamor de la guerra horroroso"<<endl;
    cout<<"siguen hoy en contraste armonioso"<<endl;
    cout<<"dulces himnos de paz y de union"<<endl;
    return 0; // no devuelve nada
}
